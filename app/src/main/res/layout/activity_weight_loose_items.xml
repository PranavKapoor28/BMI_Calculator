<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/listcolour"
        android:id="@+id/list1"
        android:elevation="22sp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/milkimage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/eggs" />

        <TextView
            android:id="@+id/WholeEggs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Whole Eggs"
            android:textColor="#FCFAFA"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#FAF7F7"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:id="@+id/list2"
        android:elevation="22sp"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/riceImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/veggies" />

        <TextView
            android:id="@+id/veggies"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Leafy vegetables"
            android:textColor="#FCFCF9F9"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#FCFBFB"
        android:elevation="22sp"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:id="@+id/list3"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/nutsImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/salmon" />

        <TextView
            android:id="@+id/Salmon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Salmon"
            android:textColor="#FAFDFCFC"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#FCFBFB"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:elevation="22sp"
        android:id="@+id/list4"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/meatImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/chicken" />

        <TextView
            android:id="@+id/Meat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Chicken Breast"
            android:textColor="#FFFEFE"
            android:textSize="24sp"
            android:textStyle="bold" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F8F6F6"
        android:elevation="22sp"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:id="@+id/list5"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/PotatoeImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/potatoes" />

        <TextView
            android:id="@+id/potatoes1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Boiled Potatoes"
            android:textColor="#FDFCFBFB"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F7F3F3"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:id="@+id/list6"
        android:elevation="22sp"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/driedImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/tuna" />

        <TextView
            android:id="@+id/Tuna"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Tuna"
            android:textColor="#FDFDFBFB"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#FDFCFC"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:elevation="22sp"
        android:id="@+id/list7"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/breadImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/beans" />

        <TextView
            android:id="@+id/beans"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Beans"
            android:textColor="#FAFDFAFA"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#FCF7F7"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:elevation="22sp"
        android:id="@+id/list8"
        android:background="@drawable/listcolour"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/avocadosImage"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:padding="5dp"
            app:srcCompat="@drawable/soup" />

        <TextView
            android:id="@+id/Soups"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="Soups"
            android:textColor="#FAFFFEFE"
            android:textSize="24sp"
            android:textStyle="bold" />

    </LinearLayout>


</LinearLayout>